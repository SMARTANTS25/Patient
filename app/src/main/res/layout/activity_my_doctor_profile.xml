<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.pifss.patient.MyDoctorProfile">


    <ImageView
        android:id="@+id/imageView48"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginLeft="14dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="68dp"
        app:srcCompat="@mipmap/doctor"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/imageView49"
        android:layout_alignStart="@+id/imageView49" />

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar12"
        style="@style/AppTheme.Toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@color/Buttons">

    </android.support.v7.widget.Toolbar>

    <TextView
        android:id="@+id/doctorNameViewDrProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageView48"
        android:layout_marginLeft="84dp"
        android:layout_marginStart="84dp"
        android:layout_toEndOf="@+id/imageView48"
        android:layout_toRightOf="@+id/imageView48"
        android:text="Doctor Name"
        android:textSize="18sp"
        android:fontFamily="monospace"/>

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_alignBottom="@+id/imageView48"
        android:layout_alignLeft="@+id/doctorNameViewDrProfile"
        android:layout_alignStart="@+id/doctorNameViewDrProfile"
        android:layout_marginBottom="16dp"
        android:textSize="14sp"
        android:fontFamily="monospace"/>

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="59dp"
        android:text="Doctor Speciality"
        android:textSize="18sp"
        android:fontFamily="monospace"
        android:layout_below="@+id/imageView48"
        android:layout_alignRight="@+id/textView17"
        android:layout_alignEnd="@+id/textView17" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        android:layout_marginRight="12dp"
        android:layout_marginTop="23dp"
        android:text="TextView"
        android:textSize="14sp"
        android:fontFamily="monospace"
        android:layout_below="@+id/textView20"
        android:layout_alignRight="@+id/textView34"
        android:layout_alignEnd="@+id/textView34" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email"
        android:layout_below="@+id/textView31"
        android:layout_alignLeft="@+id/textView20"
        android:layout_alignStart="@+id/textView20"
        android:layout_marginTop="25dp"
        android:textSize="18sp"
        android:fontFamily="monospace"/>

    <TextView
        android:id="@+id/textView33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView31"
        android:layout_alignStart="@+id/textView31"
        android:layout_below="@+id/textView32"
        android:layout_marginTop="17dp"
        android:text="TextView"
        android:textSize="14sp"
        android:fontFamily="monospace"/>

    <TextView
        android:id="@+id/textView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="Nationality"
        android:layout_below="@+id/textView33"
        android:layout_alignLeft="@+id/textView32"
        android:layout_alignStart="@+id/textView32"
        android:textSize="18sp"
        android:fontFamily="monospace"/>

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:text="TextView"
        android:layout_below="@+id/textView34"
        android:layout_alignLeft="@+id/textView33"
        android:layout_alignStart="@+id/textView33"
        android:textSize="14sp"
        android:fontFamily="monospace"/>

    <TextView
        android:id="@+id/textView36"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView34"
        android:layout_alignStart="@+id/textView34"
        android:layout_below="@+id/textView35"
        android:layout_marginTop="35dp"
        android:text="CV"
        android:textSize="18sp"
        android:fontFamily="monospace"/>

    <TextView
        android:id="@+id/textView37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="14sp"
        android:fontFamily="monospace"
        android:layout_below="@+id/textView36"
        android:layout_alignLeft="@+id/textView35"
        android:layout_alignStart="@+id/textView35" />

    <ImageView
        android:id="@+id/imageView49"
        android:layout_width="30dp"
        android:layout_height="30dp"
        app:srcCompat="@mipmap/stethoscope"
        android:layout_alignTop="@+id/textView20"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="23dp"
        android:layout_marginStart="23dp" />

    <ImageView
        android:id="@+id/imageView50"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignEnd="@+id/imageView49"
        android:layout_alignRight="@+id/imageView49"
        android:layout_alignTop="@+id/textView32"
        app:srcCompat="@mipmap/email" />

    <ImageView
        android:id="@+id/imageView52"
        android:layout_width="30dp"
        android:layout_height="30dp"
        app:srcCompat="@mipmap/big_flag_icon"
        android:layout_alignTop="@+id/textView34"
        android:layout_alignLeft="@+id/imageView50"
        android:layout_alignStart="@+id/imageView50" />

    <ImageView
        android:id="@+id/imageView53"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_above="@+id/textView37"
        android:layout_alignEnd="@+id/imageView52"
        android:layout_alignRight="@+id/imageView52"
        app:srcCompat="@mipmap/medical_report_icon" />

</RelativeLayout>
